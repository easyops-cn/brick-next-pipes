{
  "name": "@easyops-cn/brick-next-pipes",
  "version": "0.3.3",
  "description": "Pipes for Brick Next",
  "repository": "easyops-cn/brick-next-pipes",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "prestart": "rimraf dist",
    "start": "concurrently -k -n esm,cjs \"npm run start:esm\" \"npm run start:cjs\"",
    "start:esm": "tsc --watch",
    "start:cjs": "tsc --watch --project tsconfig.cjs.json",
    "prebuild": "rimraf dist",
    "build": "concurrently -n esm,cjs \"npm run build:esm\" \"npm run build:cjs\"",
    "build:esm": "tsc",
    "build:cjs": "tsc --project tsconfig.cjs.json",
    "build:docs": "api-extractor run && npm run docs:gen",
    "build:docs:dev": "api-extractor run --local --verbose && npm run docs:gen",
    "docs:gen": "api-documenter markdown --input-folder temp --output-folder docs",
    "coveralls": "coveralls < .coverage/lcov.info",
    "deploy": "gh-pages -d docs -r https://$GH_TOKEN@github.com/easyops-cn/brick-next-pipes.git",
    "release": "standard-version"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@microsoft/api-documenter": "^7.8.39",
    "@microsoft/api-extractor": "^7.9.11",
    "@types/jest": "^26.0.9",
    "@types/js-yaml": "^3.12.5",
    "@types/lodash": "^4.14.159",
    "@types/lolex": "^5.1.0",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "babel-jest": "^26.3.0",
    "concurrently": "^5.3.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "gh-pages": "^3.1.0",
    "husky": "^4.2.5",
    "jest": "^26.4.0",
    "js-yaml": "^3.14.0",
    "lint-staged": "^10.2.11",
    "lodash": "^4.17.19",
    "lolex": "^6.0.0",
    "moment": "^2.27.0",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "standard-version": "^9.0.0",
    "typescript": "^4.0.0"
  },
  "peerDependencies": {
    "js-yaml": "^3.14.0",
    "lodash": "^4.17.19",
    "moment": "^2.27.0"
  }
}
