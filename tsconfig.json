{
  "compilerOptions": {
    "declaration": true,
    "sourceMap": true,
    "importHelpers": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "target": "ESNext",
    "module": "ESNext",
    "skipLibCheck": true,
    "strict": true,
    "strictNullChecks": false,
    "outDir": "dist"
  },
  "include": ["src"]
}
